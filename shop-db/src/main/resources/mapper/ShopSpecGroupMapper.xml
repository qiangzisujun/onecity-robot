<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tangchao.shop.mapper.ShopSpecGroupMapper">

    <insert id="insertSelectiveGroup" useGeneratedKeys = "true" keyProperty="id" parameterType="com.tangchao.shop.pojo.ShopSpecGroup">
        insert into shop_spec_group(type_id,name,status,create_time,type_name) value (#{typeId},#{name},#{status},#{createTime},#{typeName})
    </insert>
</mapper>